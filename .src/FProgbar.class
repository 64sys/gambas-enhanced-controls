Public Sub CheckBox1_Click() 'lorsque la CheckBox1 est cochée (ou décochée)

  Dim i As Integer

  While CheckBox1.Value = True 'tant que la CheckBox1 est cochée
    'augmente les ProgressBarText
    For i = 0 To 100 'pour i compte de 0 à 100
      Slider1.Value = i 'le slider prend la valeur de i
      $bmd = False 'positionne $bmd sur False
      Wait 0.03 'utilisée pour l'affichage et la temporisation
    Next
    'diminue les ProgressBarText
    For i = 100 To 0 Step -1 'pour i décompte de 100 à 0 par palier de 1
      Slider1.Value = i 'le slider prend la valeur de i
      $bmd = True 'positionne $bmd sur True
      Wait 0.03 'utilisée pour l'affichage et la temporisation
    Next
  Wend

End

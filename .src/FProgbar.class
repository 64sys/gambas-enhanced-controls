Public Sub Form_KeyPress() 'control l'appui des touches

  If Key.Shift Then Inc Slider1.Value 'appuyer sur [Shift] = incrémente la valeur du slider de 1
  If Key.Control Then Dec Slider1.Value 'appuyer sur [Ctrl] = décrémente la valeur du slider de 1

End

Public Sub Form_Open() 'à l'ouverture du formulaire

  Dim i As Integer

  For i = 0 To ProgressBarText70.H / 10 'sert au dessin des pointillés pour les progressbartext verticaux...
    Label51.Text = Label51.Text & "." & Chr(10)
    Label52.Text = Label52.Text & "." & Chr(10)
  Next

  Slider1.Value = 50 'attrbue une valeur initiale au slider
  Slider1_Change() 'puis on entre dans la procédure (lorsque la valeur du slider change)

End

Public Sub Slider1_Change() 'lorsque la valeur du slider change

  Dim msg As String = ("La valeur du ProgressBarText est de ") & Slider1.Value & "%"
  Dim tp As String = (Slider1.Value / 10) + 6
  Dim dizaine As Float = Slider1.Value / 10

